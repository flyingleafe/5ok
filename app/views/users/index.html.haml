%h1 Listing users
%table
  %tr
    %th Name
    %th Email
    %th Phone
    %th Password
    %th Registered at
    %th Date of birth
    %th ''
    %th ''
    %th ''

    - @users.each do |user| 
      %tr
        %td= user.name
        %td= user.email
        %td= user.phone
        %td= user.password
        %td= user.registered_at
        %td= user.date_of_birth
        %td= link_to 'Show', user
        %td= link_to 'Edit', edit_user_path(user)
        %td= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New User', new_user_path
